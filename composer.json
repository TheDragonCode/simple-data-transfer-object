{
    "name": "dragon-code/simple-data-transfer-object",
    "description": "Simple Data Transfer Objects",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "dto",
        "data-transfer-object",
        "andrey-helldar",
        "dragon-code",
        "dragon"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/simple-data-transfer-object/issues",
        "source": "https://github.com/TheDragonCode/simple-data-transfer-object"
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*",
        "dragon-code/contracts": "^2.17",
        "dragon-code/support": "^5.0",
        "symfony/http-foundation": "^4.0 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^4.3 || ^5.4 || ^6.0"
    },
    "conflict": {
        "andrey-helldar/simple-data-transfer-object": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DragonCode\\SimpleDataTransferObject\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
