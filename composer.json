{
    "name": "andrey-helldar/simple-data-transfer-object",
    "description": "Simple Data Transfer Objects",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "andrey-helldar",
        "dto",
        "data-transfer-object"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/simple-data-transfer-object/issues",
        "source": "https://github.com/andrey-helldar/simple-data-transfer-object"
    },
    "require": {
        "php": "^7.3|^8.0",
        "andrey-helldar/contracts": "^1.4.6",
        "andrey-helldar/support": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^4.3.4|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\SimpleDataTransferObject\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
